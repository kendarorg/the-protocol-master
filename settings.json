{
  "pluginsDir": "target/plugins",
  "logLevel": "INFO",
  "dataDir": "target/data",
  "apiPort": 8095,
  "plugins": {
    "report-plugin": {
      "active": true
    }
  },
  "protocols": {
    "redis-01": {
      "protocol": "redis",
      "port": 6379,
      "login": "remotelogin",
      "password": "remotepassword",
      "timeout": 30,
      "connectionString": "tcp://192.168.56.2:26379",
      "plugins": {
        "record-plugin": {
          "resetConnectionsOnStart": true
        },
        "replay-plugin": {
          "resetConnectionsOnStart": true,
          "respectCallDuration": true
        },
        "report-plugin": {
          "active": true
        },
        "publish-plugin": {
        }
      }
    },
    "mysql-01": {
      "protocol": "mysql",
      "port": 3306,
      "login": "root",
      "password": "password",
      "timeout": 30,
      "connectionString": "jdbc:mysql://192.168.56.2:23306/db",
      "plugins": {
        "mock-plugin": {
          "active": false,
          "dataDir": "target/mocks"
        }
      }
    },
    "postgres-01": {
      "protocol": "postgres",
      "port": 5432,
      "login": "remotelogin",
      "password": "remotepassword",
      "timeout": 30,
      "connectionString": "jdbc:mysql://localhost:3306",
      "plugins": {
        "mock-plugin": {
          "active": false,
          "dataDir": "target/mocks"
        }
      }
    },
    "mqtt-01": {
      "protocol": "mqtt",
      "port": 1885,
      "login": "remotelogin",
      "password": "remotepassword",
      "timeout": 30,
      "connectionString": "tcp://localhost:1885",
      "plugins": {
        "publish-plugin": {
        },
        "record-plugin": {
          "resetConnectionsOnStart": true
        },
        "replay-plugin": {
          "resetConnectionsOnStart": true,
          "respectCallDuration": true
        }
      }
    },
    "http-01": {
      "protocol": "http",
      "http": 8085,
      "https": 8485,
      "proxy": 9999,
      "ssl": {
        "der": "resource://certificates/ca.der",
        "key": "resource://certificates/ca.key",
        "cname": "C=US,O=Local Development,CN=local.org",
        "hosts": [
          "*.google.com",
          "www.google.com"
        ]
      },
      "plugins": {
        "sample-http": {
          "active": true
        },
        "error-plugin": {
          "active": false,
          "errorCode": 0,
          "errorMessage": "ERROR",
          "percentage": 0
        },
        "report-plugin": {
          "active": true
        },
        "replay-plugin": {
          "active": false,
          "replayId": "replay{timestamp}",
          "respectCallDuration": true,
          "matchSites": [
            "net_core_http",
            "net_core_rest"
          ]
        },
        "record-plugin": {
          "active": false,
          "recordSites": []
        },
        "mock-plugin": {
          "active": false,
          "dataDir": "target/mocks"
        }
      }
    },
    "amqp-01": {
      "protocol": "amqp091",
      "port": 5672,
      "login": "root",
      "password": "password",
      "timeout": 30,
      "connectionString": "tcp://192.168.56.2:25672",
      "plugins": {
        "publish-plugin": {
        },
        "record-plugin": {
          "resetConnectionsOnStart": true
        },
        "replay-plugin": {
          "resetConnectionsOnStart": true,
          "respectCallDuration": true
        }
      }
    },
    "mongo-01": {
      "protocol": "mongodb",
      "port": 6672,
      "login": "root",
      "password": "password",
      "timeout": 30,
      "connectionString": "tcp://192.168.56.2:26672",
      "plugins": {
        "publish-plugin": {
        },
        "record-plugin": {
        },
        "replay-plugin": {
          "respectCallDuration": true
        }
      }
    }
  }
}
